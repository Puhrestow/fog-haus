{
  "name": "fog.haus",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel cp_assets client html styles serve",
    "build": "sh ./build.sh",
    "cp_assets": "cp -r lib/* dev",
    "html": "pug -O data.json --pretty --watch src/pages --out dev",
    "client": "cp src/*.js dev",
    "styles": "stylus --watch src/main.styl --out dev",
    "serve": "nodemon app/index.js",
    "postinstall": "sh ./build.sh"
  },
  "engines": {
    "node": ">= 6"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^3.1.0",
    "pug": "^2.0.0-beta6",
    "stylus": "^0.54.5"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "http-server": "^0.9.0",
    "nodemailer": "^2.6.4",
    "superagent": "^2.3.0"
  }
}
